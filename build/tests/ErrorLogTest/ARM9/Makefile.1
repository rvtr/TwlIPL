MAKEROM		 = $(TWL_TOOLSDIR)/bin/makerom.TWL.secure.exe

MAKEROM_FLAGS	+=	-DERRLOGTEST_GAMECODE=$(GAMECODE)	\
					-DERRLOGTEST_MEDIA=$(MEDIA)

