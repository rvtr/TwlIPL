SUBDIRS          = $(TWL_IPL_RED_ROOT)/build/libraries_sysmenu/errorLog

GAMECODE	=	013A
MEDIA		=	NAND
TARGET_BIN       = main.tad

TARGET_FIRM		=	SYSTEMMENU
TARGET_PLATFORM	=	TWL
TWL_ARCHGEN		=	LIMITED
TWL_NANDAPP	=	TRUE
BINDIR			= 	./bin/$(TWL_BUILDTYPE)/$(MEDIA)
#----------------------------------------------------------------------------



MISC_DIR	=	../../../systemMenu_RED/misc
LINCLUDES		+=	$(MISC_DIR)/include

